<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
  </PropertyGroup>
  <PropertyGroup>
    <id>Yove.Music</id>
    <Version>1.1.2</Version>
    <Authors>Sunny</Authors>
    <NeutralLanguage>en-US, ru-RU</NeutralLanguage>
    <Description>VK Music client for .Net</Description>
    <TargetFrameworks>netstandard2.0</TargetFrameworks>
    <PackageTags>vk; music; vkmusic; boom; .net core; .net</PackageTags>
    <PackageIconUrl>https://raw.githubusercontent.com/TheSuunny/xYove/master/Splash.png</PackageIconUrl>
    <PackageProjectUrl>https://github.com/TheSuunny/Yove.Music</PackageProjectUrl>
    <PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
    <RepositoryType>git</RepositoryType>
    <RepositoryUrl>https://github.com/TheSuunny/Yove.Music</RepositoryUrl>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Yove.Http" Version="1.1.4"/>
  </ItemGroup>
</Project>